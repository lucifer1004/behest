    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "react-scripts": "2.1.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.css **/*.md **/*.json",
    "now-build": "react-scripts build && mv build dist"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
